{
  "name": "node-api-starter",
  "description": "Node REST API Starter Kit",
  "main": "''",
  "scripts": {
    "mongo": "mongod --dbpath ./data/db",
    "mongo:seed": "mongoimport --db testdb --collection favs --host Strup --drop --file ~/server/seed/favs-dataset.json",
    "dev-server": "nodemon -w server --exec \"babel-node server --presets es2015,stage-0\"",
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server --progress --profile --colors",
    "apidoc": "apidoc -i server/ -o apidoc/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mimoskal/node-api-starter"
  },
  "author": "Michal Moskal",
  "license": "MIT",
  "homepage": "https://github.com/mimoskal/node-api-starter",
  "dependencies": {
    "node-sass": "^4.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "sass-loader": "^6.0.2"
  },
  "devDependencies": {
    "apidoc-plugin-schema": "^0.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "css-loader": "0.26.1",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "extract-text-webpack-plugin": "^v2.0.0-rc.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "joi": "^10.4.1",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.9.6",
    "mongoose-keywords": "^0.3.2",
    "mongoose-unique-validator": "^1.0.5",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "querymen": "^2.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-cleanup-plugin": "^0.4.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  }
}
